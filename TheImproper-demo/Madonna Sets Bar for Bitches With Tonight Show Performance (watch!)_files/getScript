);a.asin=new RegExp("^(https?:)?//(www\\.)?amazon\\.("+b+")/(exec/obidos|o)/ASIN\\d?/(\\w{10})/(ref=[\\w]+/)?([^/?]+)","i");a.redirect=new RegExp("^(https?:)?//(www\\.)?amazon\\.("+b+")/(exec/obidos|o)/redirect/?\\?([^/?]+)","i");a.isbn=new RegExp("^(https?:)?//(www\\.)?amazon\\.("+b+")/(exec/obidos|o)/ISBN=(\\w{10})/(ref=[\\w]+/)?([^/?]+)","i");this.regEx=a;this.hotSpotTemplate={id:"__ID__",type:"IMAGE",pinLocation:{x:"50",y:"50",imgSrc:"__IMG_SRC__"},linkId:"__PS_AUTO__",asin:"__ASIN__",linkLocation:null,source:"auto"};this.minimumDimensionToFilter=50;this.$=amznpubstudio_jQuery;this.linkCount=0}amzn_ps_autobot.prototype.process=function(b){var a=this;try{if(a.filter(b)){var d=a.checkAmazonLink(b);if(d.asin!=undefined){amznPubstudioLogger.log("AutoBot - found a match, creating hotspot",b,d.asin,d.tag);a.createHotSpot(b,d);b.addClass("amzn-auto-link");a.linkCount++}}}catch(c){amznPubstudioLogger.log("AutoBot - error processing image",c)}};amzn_ps_autobot.prototype.filter=function(b){var a=this;var c=a.minimumDimensionToFilter;return((b.prop("tagName")=="IMG")&&b.attr("src")&&b.height()>=c&&b.width()>=c)};amzn_ps_autobot.prototype.checkAmazonLink=function(h){var k=this;var b=h.closest('a[href*="amazon.'+k.tld+'"]');var f,l=null;if(b.length==1){var a=b.attr("href");var d=b.get(0).search;var c=d.match(/tag=([^&]+)/);if(c){l=c[1];var g;if(g=a.match(k.regEx.product)){f=g[1]}else{if(g=a.match(k.regEx.asin)){f=g[5]}else{if(g=a.match(k.regEx.redirect)){var j=g[5];var i;if(i=j.match("path=ASIN/(\\w{10})")){f=i[1]}}else{if(g=a.match(k.regEx.isbn)){f=g[5]}}}}}}if(f){b.addClass("amzn-pp-ignore");if(b.hasClass("amzn-pp-active")&&window.amznJQ&&window.amznJQ.jQuery){var e=window.amznJQ.jQuery(b.get(0));if(typeof e.removeAmazonPopoverTrigger=="function"){e.removeAmazonPopoverTrigger()}}}return{asin:f,tag:l}};amzn_ps_autobot.prototype.createHotSpot=function(c,e){var b=this;var f=e.asin;var a=e.tag;var g=b.adView.getSrc(c);var d=b.$.extend(true,{},b.hotSpotTemplate);d.id=amznPubStudioBootStrap.generateUrlHash()+"_"+new Date().getTime();d.pinLocation.imgSrc=g;d.asin=f;d.tag=a;d.linkId=b.generateLinkId(g,f);b.adView.storeHotSpot(d)};amzn_ps_autobot.prototype.generateLinkId=function(f,d){var e="NA";try{var a=amznPubStudioBootStrap;var c=a.domainId.replace(/\./g,"");var g=c.split("-");e="auto-ps"+g[g.length-1].substr(0,12)+a.generateUrlHash().toString(16).substr(0,8)+Math.abs(a.getHash(f)).toString(16).substr(0,5)}catch(b){amznPubstudioLogger.log("AutoBot - error in linkId generation",b)}return e};var amzn_ps_cssfilename = 'aps-base.min.css'; var amzn_ps_toolbarenabled = false;

// context handshake between the script and extension
var extensionContextHandshake = function($Ctx, view, admin) {
    if(($Ctx.browser === 'chrome' || $Ctx.browser === 'ff') && $Ctx.locale==='US') {
        //listener to disable view
        document.addEventListener('AmznPsDisableView', function () {
            //implies extension is present and signed-in for the domain
            view.disable(true);
            if(admin) admin.removeLoginStickyHeader();
            document.dispatchEvent(new CustomEvent('AmznPsViewDisabled'));
        });
        var broadcastContext = function () {
            document.dispatchEvent(new CustomEvent('AmznPsContext', {
                detail: $Ctx
            }));
        };
        document.addEventListener('AmznPsHandshakeAcknowledge', function(){
            //implies extension is present, but not yet signed-in
            //show toolbar with disabled state asking publisher to log-in
            if(admin) admin.showLoginStickyHeader();
        });
        document.addEventListener('AmznPsHandshake', function(){
            broadcastContext();
        });
        broadcastContext();
    }
};
function amzn_pubstudio_bootstrap(){this.DOMNodeRemovedTested=false;this.DOMNodeRemovedPresent=false;this.isMobileAgent=false;this.isTabletAgent=false;this.keyVerificationDelay=200;this.useLocalStorage=false;this.isExperimentOn=false;this.amznUseRelativeUrlsForImages=false;this.loadedUrls={};this.urlHashMap={};this.verificationPending="PENDING_VERIFICATION";this.supportedBrowsers={chrome:true,ff:true}}amzn_pubstudio_bootstrap.prototype.getBrowser=function(){var isOpera=!!window.opera||navigator.userAgent.indexOf(" OPR/")>=0;if(isOpera){return"opera"}var isFirefox=typeof InstallTrigger!=="undefined";if(isFirefox){return"firefox"}var isSafari=Object.prototype.toString.call(window.HTMLElement).indexOf("Constructor")>0;if(isSafari){return"safari"}var isChrome=!!window.chrome&&!isOpera;if(isChrome){return"chrome"}var isIE=
/*@cc_on!@*/
false||!!document.documentMode;if(isIE){return"ie"}};amzn_pubstudio_bootstrap.prototype.checkUserAgent=function(){var a=this;var b=navigator.userAgent.toLocaleLowerCase();if(/iphone/i.test(b)||/ipod/i.test(b)||/windows phone/i.test(b)||/symbian/i.test(b)||(/blackberry/i.test(b)&&/mobile/i.test(b))||(/android/i.test(b)&&/mobile/i.test(b))){amznPubStudioBootStrap.isMobileAgent=true}else{if(/ipad/i.test(b)||/kindle/i.test(b)||(/blackberry/i.test(b)&&!/mobile/i.test(b))||(/android/i.test(b)&&!/mobile/i.test(b))||/tablet/i.test(b)){amznPubStudioBootStrap.isTabletAgent=true}}};amzn_pubstudio_bootstrap.prototype.init=function(){var a=this;a.checkUserAgent();amznPubstudioServiceSettings.initServerUrls();if(a.autotaggingEnabled){window.amznPubstudioAutoTagger=new amznps_autoTag();amznPubstudioAutoTagger.init()}if(window.amznps_disablePubstudio){amznPubstudioLogger.log("amznps_disablePubstudio flag is set to true, so disabling publisher studio");return}a.loadCSS(this.cssUrl+"/"+amzn_ps_cssfilename);window.amznpsMetricsModule=new amzn_pubstudio_metrics();a.initComponents()};amzn_pubstudio_bootstrap.prototype.initComponents=function(){var b=this;amznPubstudioLogger.log("Initializing components");var f=navigator.userAgent.toLocaleLowerCase();this.opera=amznpubstudio_jQuery.browser.opera;this.ff=/firefox/.test(f);this.safari=Object.prototype.toString.call(window.HTMLElement).indexOf("Constructor")>0;this.chrome=!this.safari&&amznpubstudio_jQuery.browser.webkit;this.ie=f.indexOf("msie")>0||f.match(/trident.*rv\:11\./);window.amznPubstudioViewPin=new amzn_pubstudio_view_pin();if(b.isAutoBotOn){b.autoBot=new amzn_ps_autobot(window.amznPubstudioViewPin);amznPubstudioLogger.log("AutoBot is initialized into bootstrap")}if(amzn_ps_toolbarenabled){window.amznPubstudioAdminPin=new amzn_pubstudio_admin_pin(amznPubstudioViewPin);amznpubstudio_jQuery(window).bind("beforeunload",function(){if(window.amznPubstudioAdminPin){window.amznPubstudioAdminPin.storeToolbarState()}})}var a=b.isMobileAge